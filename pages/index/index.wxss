/* pages/index/index.wxss */
image {
  width: 50rpx;
  height: 50rpx;
  padding:30rpx;
}
view.clock {
  padding: 20px 20px 10px;
  text-align: center;
}
view.recent {
  padding: 10px 10px 10px;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}
view.kind-list {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
view.item {
  width: 90%;
  height: 120rpx;
  background-color: #cccccc;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}